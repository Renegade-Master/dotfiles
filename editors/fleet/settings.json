{
    "autoSave": true,
    "editor.guides": [
        72,
        80,
        120
    ],
    "editor.indentStyle": "Spaces",
    "editor.renderWhitespace": "All",
    "editor.showLineStatus": true,
    "editor.showTabs": true,
    "git.revert.confirm": true,
    "keymap": "intellij",
    "launchTerminalOnOpen": true,
    "showShortcuts": true,
    "terminal.fontFamily": "JetBrains Mono",
    "terminal.profiles": [
        {
            "name": "ZSH",
            "program": "zsh"
        },
        {
            "name": "PowerShell 7",
            "program": "pwsh"
        },
        {
            "name": "Nu Shell",
            "program": "nu"
        }
    ],
    "theme": "sync-with-os",
    "trimTrailingSpacesOnSave": true,
    "plugins": [
        {
            "type": "remove",
            "pluginName": "fleet.aiCompletion"
        },
        {
            "type": "remove",
            "pluginName": "fleet.go.ai"
        },
        {
            "type": "remove",
            "pluginName": "fleet.java.ai"
        },
        {
            "type": "remove",
            "pluginName": "fleet.javascript.ai"
        },
        {
            "type": "remove",
            "pluginName": "fleet.kotlin.ai"
        },
        {
            "type": "remove",
            "pluginName": "fleet.python.ai"
        },
        {
            "type": "remove",
            "pluginName": "fleet.rust.ai"
        },
        {
            "type": "remove",
            "pluginName": "fleet.ai"
        },
        {
            "type": "remove",
            "pluginName": "fleet.keymaps.VSCode"
        },
        {
            "type": "remove",
            "pluginName": "fleet.wsl"
        }
    ]
}
